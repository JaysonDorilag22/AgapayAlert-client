{
  "expo": {
    "name": "AgapayAlert-client",
    "slug": "AgapayAlert-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#EEEEEE",
      "image": "./assets/splash-icon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.agapayalert.client",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLSchemes": [
          "com.googleusercontent.apps.1061555533518-k9vf1ro6q9tdu7o47091rq1mfpu1i3n2"
        ]
      }
    },
    "android": {
      "package": "com.agapayalert.client",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png"
      }
    },
    "web": {
      "favicon": "./assets/splash-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "841fe1db-7c33-428b-ada7-011aa419493b"
      },
       "oneSignalAppId": "4948d711-5abb-437c-bd1f-879f45c8e3c5"
    }
  }
}